site_name: "[[ package_name ]]"
site_description: "[[ project_description ]]"
dev_addr: localhost:9090

nav:
- Home:
    - Overview: index.md
- API Doc: api_doc.md

# https://python-markdown.github.io/extensions/
markdown_extensions:
- toc:
    permalink: True
- plantuml_markdown:
    server: http://www.plantuml.com/plantuml

plugins:
- search
- mkdocstrings:
    watch:
    - [[ package_name ]]
- gen-files:
    scripts:
    - docs/gen_api_doc.py